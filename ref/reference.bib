  author       = {Yang, Yuqi and Wang, Shanshan and Wang, Guopeng and Lian, Yuke
                  and Xue, Lingfeng and Jiang, Wenhong and Guo, Qiang and Song,
                  Chen and Li, Long},
  journaltitle = {Nat. Struct. Mol. Biol.},
  publisher    = {Springer Science and Business Media LLC},
  date         = {2025-08-28},
  abstract     = {The mitochondrial translocase of the outer membrane (TOM) and
                  translocase of the inner membrane 23 (TIM23) complexes are
                  coupled to control protein import across the outer and inner
                  membranes, respectively. However, the mechanisms of protein
                  recognition and sorting in the TOM-TIM23 pathway remain
                  unclear. Here we report cryo-electron microscopy structures of
                  a translocating polypeptide substrate captured in the active
                  TOM-TIM23 supercomplex from Saccharomyces cerevisiae. In the
                  TOM complex, the polypeptide substrate adopts multiple
                  conformations stabilized by hydrophilic residues from distinct
                  regions of the Tom40 channel. In the TIM23 complex, the Tim17
                  and Mgr2 subunits create the translocation pathway, with a
                  central restriction formed by four highly conserved
                  hydrophobic residues. The substrate primarily interacts with
                  hydrophobic residues along the Tim17-Mgr2 pathway. Substrate
                  hydrophobicity modulates the association of Mgr2 with Tim17,
                  enabling dynamic regulation of protein sorting toward either
                  the matrix or membrane. These findings reveal a sophisticated
                  translocation mechanism of the TOM-TIM23 supercomplex that
                  ensures the efficient import of diverse mitochondrial
                  proteins.},
  language     = {en}
}

@ARTICLE{Verburgt2025-ut,
