@ARTICLE{Dona2025-ci,
  title        = {The {SAFE} Labs Handbook as a tool for improving lab culture},
  author       = {Donà, Erika and Gahan, James M and Lau, Petrina and Jeschke,
                  Jana and Ott, Torben and Reinhard, Katja and Sinigaglia,
                  Chiara and Treur, Jorien L and Vogl, Thomas and Bugeon,
                  Stephane and Mariotti, Letizia and Rossi, L Federico and Coen,
                  Philip},
  journaltitle = {Elife},
  publisher    = {eLife Sciences Publications, Ltd},
  volume       = {14},
  issue        = {e108853},
  date         = {2025-11-28},
  abstract     = {Creating positive and equitable lab environments has become a
                  growing priority for the scientific community and funders of
                  scientific research. Research institutions typically respond
                  to this need by providing mandatory or optional training
                  opportunities for their staff. However, there are limited
                  resources for group leaders to improve the culture in their
                  labs. Here, we introduce the SAFE Labs Handbook: a collection
                  of 30 "commitments" that can be implemented by individual
                  group leaders to improve research culture in the life
                  sciences. The commitments were collaboratively developed by 13
                  group leaders working in eight different European countries.
                  We also report the results of a survey in which we asked more
                  than 200 researchers, at various career stages, about the
                  commitments. Even though all 30 commitments were rated as
                  significantly important by respondents, implementation rates
                  were notably low (<25\%). However, more than 95\% of group
                  leaders said they would consider implementing them. The SAFE
                  Labs Handbook therefore represents a unique, community-driven
                  tool with the potential to improve lab culture on a global
                  scale.},
  keywords     = {early-career researchers; equality; group leaders; lab
                  handbooks; none; research culture},
  language     = {en}
}

